import java.util.Scanner;

public class ifelse1 {
  public static void main(String[] args) {
    // int age = 18;
    Scanner sc = new Scanner(System.in);
    int age = sc.nextInt();
    if (age >= 18) {
      System.out.println("he can be drive");
    } else {
      System.out.println("not eligible");
    }

  }
}
